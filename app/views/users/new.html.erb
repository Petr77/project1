<%= form_for @user do |u| %>
  <p>
    <%= u.label :email %>
    <%= u.text_field :email %>
  </p>
  <p>
    <%= u.label :password %>
    <%= u.password_field :password %>
  </p>
  <p>
  </p>
  <p>
    <%= u.label :name %>
    <%= u.text_field :name %>
  </p>
  <p>
  </p>
  <p>
    <%= u.label :date_of_birth %>
    <%= u.date_field :dob %>
  </p>
  <p>
    <%= u.submit "Signup" %>
  </p>
<%end%>
